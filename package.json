{
  "name": "regex-context",
  "displayName": "regex-context",
  "description": "Apply regex in a given context",
  "publisher": "byronwall",
  "repository": "https://github.com/byronwall/regex-context",
  "license": "MIT",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:regex-context.helloWorld",
    "onView:regex-context.colorsView"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "regex": [
        {
          "type": "webview",
          "id": "regex-context.colorsView",
          "name": "Regex Context"
        }
      ]
    },
    "commands": [
      {
        "command": "regex-context.helloWorld",
        "title": "Hello World"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "regex",
          "title": "Regex",
          "icon": "resources/icons/regex.svg"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -b src",
    "watch": "tsc -b src -w",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "publish": "npx vsce publish"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.63.0",
    "@types/ejs": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "ejs": "^3.1.6"
  }
}
